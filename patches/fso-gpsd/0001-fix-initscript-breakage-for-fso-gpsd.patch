From 9ee90f8f3e96c4f589270ce2883ba5e418f693bf Mon Sep 17 00:00:00 2001
From: Klaus Kurzmann <mok@fluxnetz.de>
Date: Sun, 28 Dec 2008 10:36:11 +0100
Subject: [PATCH] fix initscript breakage for fso-gpsd

Signed-off-by: Klaus Kurzmann <mok@fluxnetz.de>
---
 packages/freesmartphone/fso-gpsd/fso-gpsd |    2 +-
 1 files changed, 1 insertions(+), 1 deletions(-)

diff --git a/packages/freesmartphone/fso-gpsd/fso-gpsd b/packages/freesmartphone/fso-gpsd/fso-gpsd
index f7b0196..4121d95 100644
--- a/packages/freesmartphone/fso-gpsd/fso-gpsd
+++ b/packages/freesmartphone/fso-gpsd/fso-gpsd
@@ -14,7 +14,7 @@ NAME=fso-gpsd
 case "$1" in
     start)
         echo -n "Starting freesmartphone.org gpsd compatibility daemon: "
-        start-stop-daemon --start /usr/sbin/fso-gpsd -S localhost:gpsd -P /var/run/${NAME}.pid
+        start-stop-daemon --start --exec /usr/sbin/fso-gpsd -- -S localhost:gpsd -P /var/run/${NAME}.pid
 
         if [ $? = 0 ]; then
             echo "(ok)"
-- 
1.6.0.6

